CREATE TABLE usuario (
    codigo char(7) NOT NULL,
    nome varchar(255) NOT NULL,
    idade char(2) NOT NULL,
    email varchar(255),
    sexo varchar(15),
    curso varchar(30),
    conhecimento varchar(2000),
    PRIMARY KEY (codigo)
);

CREATE TABLE login (
    codigo char(7) NOT NULL,
    nome varchar(255) NOT NULL,
    senha varchar(50) NOT NULL,
    PRIMARY KEY (codigo)
);